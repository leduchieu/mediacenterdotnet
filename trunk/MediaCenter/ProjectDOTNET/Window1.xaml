		</EventTrigger>
		<EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="video">
			<BeginStoryboard Storyboard="{StaticResource videoBoard2}" x:Name="videoBoard2_BeginStoryboard1"/>
		</EventTrigger>
		<EventTrigger RoutedEvent="Mouse.MouseEnter" SourceName="RSS">
			<BeginStoryboard Storyboard="{StaticResource RSSBoard}" x:Name="RSSBoard_BeginStoryboard1"/>
		</EventTrigger>
		<EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="RSS">
			<BeginStoryboard Storyboard="{StaticResource RSSBoard2}" x:Name="RSSBoard2_BeginStoryboard1"/>
