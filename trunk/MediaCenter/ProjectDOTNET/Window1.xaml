			<BeginStoryboard Storyboard="{StaticResource RSSBoard}" x:Name="RSSBoard_BeginStoryboard"/>
			<BeginStoryboard Storyboard="{StaticResource RSSBoard2}" x:Name="RSSBoard2_BeginStoryboard"/>
			<BeginStoryboard Storyboard="{StaticResource creditsBoard}" x:Name="creditsBoard_BeginStoryboard"/>
			<BeginStoryboard Storyboard="{StaticResource creditsBoard2}" x:Name="creditsBoard2_BeginStoryboard"/>
			<BeginStoryboard Storyboard="{StaticResource phoneBoard}" x:Name="phoneBoard_BeginStoryboard"/>
			<BeginStoryboard Storyboard="{StaticResource phoneBoard2}" x:Name="phoneBoard2_BeginStoryboard"/>
			<BeginStoryboard Storyboard="{StaticResource windowLoad}" x:Name="windowLoad_BeginStoryboard"/>
		</EventTrigger>
		<EventTrigger RoutedEvent="Mouse.MouseEnter" SourceName="music">
			<BeginStoryboard Storyboard="{StaticResource musicBoard}" x:Name="musicBoard_BeginStoryboard1"/>
		</EventTrigger>
		<EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="music">
			<BeginStoryboard Storyboard="{StaticResource musicBoard2}" x:Name="musicBoard2_BeginStoryboard1"/>
		</EventTrigger>
		<EventTrigger RoutedEvent="Mouse.MouseEnter" SourceName="video">
