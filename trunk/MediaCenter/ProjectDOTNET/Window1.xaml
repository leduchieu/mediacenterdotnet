		<EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="images">
			<BeginStoryboard Storyboard="{StaticResource imagesBoard2}" x:Name="imagesBoard2_BeginStoryboard"/>
		</EventTrigger>
		<EventTrigger RoutedEvent="FrameworkElement.Loaded">
			<BeginStoryboard Storyboard="{StaticResource closeLeave}" x:Name="closeLeave_BeginStoryboard"/>
			<BeginStoryboard Storyboard="{StaticResource imagesBoard}" x:Name="imagesBoard_BeginStoryboard1"/>
			<BeginStoryboard Storyboard="{StaticResource imagesBoard2}" x:Name="imagesBoard2_BeginStoryboard1"/>
			<BeginStoryboard Storyboard="{StaticResource musicBoard}" x:Name="musicBoard_BeginStoryboard"/>
