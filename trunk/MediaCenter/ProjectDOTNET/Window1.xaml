		</EventTrigger>
		<EventTrigger RoutedEvent="Mouse.MouseEnter" SourceName="telefono">
			<BeginStoryboard Storyboard="{StaticResource phoneBoard}" x:Name="phoneBoard_BeginStoryboard1"/>
		</EventTrigger>
		<EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="telefono">
			<BeginStoryboard Storyboard="{StaticResource phoneBoard2}" x:Name="phoneBoard2_BeginStoryboard1"/>
		</EventTrigger>
		<EventTrigger RoutedEvent="Mouse.MouseLeave" SourceName="Credits">
			<BeginStoryboard Storyboard="{StaticResource creditsBoard2}" x:Name="creditsBoard2_BeginStoryboard1"/>
		</EventTrigger>
		<EventTrigger RoutedEvent="FrameworkElement.Loaded">
			<BeginStoryboard Storyboard="{StaticResource windowLoad}" x:Name="windowLoad_BeginStoryboard1"/>
		</EventTrigger>
	</Window.Triggers>

